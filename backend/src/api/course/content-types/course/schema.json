{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "speaker_name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "course_name": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "thumbnail_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "thumbnail_video": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "videos"
      ]
    },
    "description": {
      "type": "richtext"
    },
    "episodes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::episode.episode",
      "mappedBy": "course"
    },
    "publish_date": {
      "type": "datetime"
    },
    "asset_download": {
      "type": "string"
    },
    "course_detail": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::course-detail.course-detail",
      "inversedBy": "course"
    }
  }
}
